//- form을 p 나 div 등으로 바꾸기

DOCTYPE
html
  head
    meta(charset="utf-8")
    title Sleep Log Controller
    link(rel="stylesheet" href="./stylesheets/style.css")
  body
    div(class="container")
      div
        button(onclick="goodNight()" class="btn-time-stamp") Good Night
        button(onclick="goodMorning()" class="btn-time-stamp") Good Moning
      div(class="clock-container")
        span(class="clock clock-left" id="start") 
        span(class="clock clock-right" id="end")
      div
        p Recent night
        p(id="recent-sleep") 08:12
      div
        button(class="btn-show-graph") Weeks
        button(class="btn-show-graph") Months
  script(type="text/javascript" src="./javascripts/sleepLogController.js")
